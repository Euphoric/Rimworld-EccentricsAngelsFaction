        <defName>Crafting</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>4</amount>
      </li>
    </skillGains>
    <requiredWorkTags>
      <li>Crafting</li>
      <li>Caring</li>
    </requiredWorkTags>
    <forcedTraits>
      <li>
        <defName>Transhumanist</defName>
        <chance>50</chance>
      </li>
    </forcedTraits>
    <disallowedTraits>
      <li>
        <defName>BodyPurist</defName>
        <chance>100</chance>
      </li>
    </disallowedTraits>
    <spawnCategories>
      <li>EccentricsStaff</li>
      <li>EccentricsTechnician</li>
    </spawnCategories>
  </AlienRace.BackstoryDef>

