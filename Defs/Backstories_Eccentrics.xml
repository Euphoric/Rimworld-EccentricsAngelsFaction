  <AlienRace.BackstoryDef ParentName="BaseEccentricsBackstory">
    <defName>Eccentrics_Staff</defName>
    <title>staff</title>
    <titleShort>staff</titleShort>
    <baseDescription>Hauling, cleaning, running errands, [PAWN_nameDef] did all the menial tasks to keep the base running effectively.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Animals</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Plants</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Mining</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>1</amount>
      </li>
    </skillGains>
    <requiredWorkTags>
